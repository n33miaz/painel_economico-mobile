{
  "expo": {
    "name": "painel-economico-br",
    "slug": "painel-economico-br",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#053D99"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-splash.png",
        "backgroundColor": "#053D99"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo-splash.png"
    },
    "plugins": ["expo-font"]
  }
}
